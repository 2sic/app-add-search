@inherits Custom.Hybrid.Razor14
@using ToSic.Razor.Blade;

@{

  var data = new {
    wrapperAttribute = DynamicModel.WrapperAttribute
  };

}

@* Include bootstrap 5 if the framework isn't known - then this file is still called, and we add Bootstrap to the page *@
@Kit.Page.Activate(condition: Kit.Css.IsUnknown, features: "Bootstrap5")


@if (DynamicModel.ResultPage == true) {
  <script src="https://addsearch.com/js/?key=@App.Settings.SearchKey&type=resultpage"></script>
  return;
}

@Kit.Page.TurnOn("window.appSearch3.init()", data: data)

@* Include styles & scripts of the App *@
<link rel="stylesheet" href="@CmsContext.View.Path/dist/styles.min.css" @Kit.Page.AssetAttributes()/>
<script type="text/javascript" src="@CmsContext.View.Path/dist/scripts.min.js" @Kit.Page.AssetAttributes(position: "bottom")></script>


